eC = {\n  // state: {\n  //   count: 3\n  // },\n  // 資料保留\n  state: () => ({\n    count: 3\n  }),\n  // 資料不保留\n  mutations: {\n    ADD_COUNT (state) {\n      state.count += 4\n    }\n  }\n}\n\nexport default {\n  data () {\n    return {\n      hasModule: false\n    }\n  },\n  computed: {\n    ...mapState({\n      count: state => state.count\n    })\n  },\n  methods: {\n    toggleHandler () {\n      if (this.$store.hasModule('c')) {\n        this.$store.unregisterModule('c', moduleC)\n        this.hasModule = false\n      } else {\n        this.$store.registerModule('c', moduleC)\n        this.hasModule = true\n      }\n    }\n  }\n}\n</script>\n"]}]}