>\n    <!-- solution -->\n\n    <input class=\"button\" type=\"submit\" value=\"Submit\">\n\n  </form>\n</template>\n\n<script>\nexport default {\n  name: 'ReviewForm',\n  data () {\n    return {\n      name: '',\n      review: '',\n      rating: null,\n      recommend: null\n    }\n  },\n  methods: {\n    onSubmit () {\n      if (this.name === '' || this.review === '' || this.rating === null || this.recommend === null) {\n        alert('Review is incomplete. Please fill out every field.')\n        return\n      }\n\n      const productReview = {\n        name: this.name,\n        review: this.review,\n        rating: this.rating,\n        recommend: this.recommend\n\n      }\n      this.$emit('review-submitted', productReview)\n\n      this.name = ''\n      this.review = ''\n      this.rating = null\n      this.recommend = null\n    }\n  }\n}\n</script>\n"]}]}