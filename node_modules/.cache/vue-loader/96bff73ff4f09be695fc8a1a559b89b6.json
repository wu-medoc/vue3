//   });\n\nexport default {\n  name: 'Basic',\n  data () {\n    return {\n      currentTab: 'Home',\n      tabs: ['Home', 'Posts', 'Archive']\n    }\n  },\n  computed: {\n    currentTabComponent: function () {\n      return 'tab-' + this.currentTab.toLowerCase()\n    }\n  }\n}\n</script>\n\n<style>\n    .tab-button {\n    padding: 6px 10px;\n    border-top-left-radius: 3px;\n    border-top-right-radius: 3px;\n    border: 1px solid #ccc;\n    cursor: pointer;\n    background: #f0f0f0;\n    margin-bottom: -1px;\n    margin-right: -1px;\n    }\n    .tab-button:hover {\n    background: #e0e0e0;\n    }\n    .tab-button.active {\n    background: #e0e0e0;\n    }\n    .tab {\n    border: 1px solid #ccc;\n    padding: 10px;\n    display: inherit;\n    }\n</style>\n"]}]}