urn {\n      input: ''\n    }\n  },\n  mounted () {\n    if (!this.content) return this.$router.replace({ path: '/vue3/Api/' })\n    this.input = this.content.content\n    // this.input = this.$store.state.contents.filter((item) => {\n    //   return item.id === this.$route.params.id\n    // })[0].content\n  },\n  computed: {\n    content () {\n      return this.$store.state.contents.filter((item) => {\n        return item.id === parseInt(this.$route.params.id)\n      })[0]\n    }\n  },\n  methods: {\n    updateHandler () {\n      this.$store.dispatch('contentUpdate', {\n        id: this.$route.params.id,\n        input: this.input\n      }).then(() => {\n        this.$router.replace({ path: '/vue3/Api/' })\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}