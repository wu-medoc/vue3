=\"{ fontSize: postFontSize + 'em' }\">\n        <blog-post\n          v-for=\"post in posts\"\n          :key=\"post.id\"\n          :title=\"post.title\"\n          @enlarge-text=\"postFontSize += 0.1\"\n        ></blog-post>\n      </div>\n    </div>\n</template>\n<script>\nexport default {\n  name: 'Blog',\n  data () {\n    return {\n      posts: [\n        { id: 1, title: 'My journey with Vue' },\n        { id: 2, title: 'Blogging with Vue' },\n        { id: 3, title: 'Why Vue is so fun' }\n      ],\n      postFontSize: 0.5\n    }\n  },\n  props: ['post'],\n  methods: {\n    onEnlargeText: function (enlargeAmount) {\n      this.postFontSize += enlargeAmount\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}